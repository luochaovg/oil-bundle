{% extends '@LeonBsw/foundation/w3c.html.twig' %}

{% block w3c %}
  {% if limbs_innocence ?? true %}
    {% include '@LeonBsw/limbs/innocence.html.twig' %}
  {% endif %}
  <div class="{{ ['bsw-body', tpl_body_class ?? null]|filter(v => v)|join(' ') }}">
    <template>
      {% if limbs_init ?? true %}
        {% include '@LeonBsw/limbs/init.html.twig' %}
      {% endif %}
      {{ (modal_html ?? null)|raw }}
      {% block body %}{% endblock %}
    </template>
  </div>
{% endblock %}
