bsw.configure({
    data: {},
    method: {
        chat_tabs_switch(key) {
            this.redirect(this.getBswData($(`#${key}`)));
        },
    },
    logic: {
        chart_init(v) {
            {% for item in chart.items %}
              bsw.chart({
                  id: '{{ item.name }}',
                  theme: '{{ item.theme }}',
                  option: {{ item.optionStringify|raw }}
              });
            {% endfor %}
        }
    }
});