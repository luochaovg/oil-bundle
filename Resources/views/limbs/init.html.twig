<p class="app-hide" v-init:configure='{{ (scaffold.json ?? '{}')|raw }}'></p>
<p class="app-hide" v-init:message='{{ (scaffold.msg ?? '{}')|raw }}'></p>
<p class="app-hide" v-init:tips='{{ (scaffold.tips ?? '{}')|raw }}'></p>
{% for key, url in tpl_api ?? [] %}
  <p class="app-hide" v-init:api_{{ key }}=`{{ url }}`></p>
{% endfor %}