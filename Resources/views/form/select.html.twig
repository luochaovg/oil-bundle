{% if form.valueMultiple %}
  {% set initial = (form.value is same as(null)) ? null : ("initialValue: " ~ form.value ~ ",") %}
{% else %}
  {% set initial = (form.value is same as(null)) ? null : ("initialValue: `" ~ form.value ~ "`,") %}
{% endif %}
<a-select
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  name="{{ form.name }}"
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  size="{{ form.size }}"
  :allow-clear="{{ form.allowClear ? 'true' : 'false' }}"
  :label-in-value="{{ form.labelInValue ? 'true' : 'false' }}"
  :default-active-first-option="true"
  :show-search="{{ form.showSearch ? 'true' : 'false' }}"
  option-filter-prop="{{ form.optionFilterProp }}"
  :show-arrow="{{ form.showArrow ? 'true' : 'false' }}"
  :token-separators="{{ form.tokenSeparators }}"
  mode="{{ form.mode }}"
  not-found-content="{{ form.notFoundContent }}"
  :dropdown-match-select-width="false"
  :dropdown-menu-style="{{ form.dropdownStyle }}"
  {% if logic.scene == 'persistence' %}
    @click.native="persistenceSwitchField = '{{ field }}'"
  {% endif %}
  {% if form.switchFieldShape %}
    @change="switchFieldShapeWithSelect"
  {% endif %}
  placeholder="{{ form.placeholder }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ field }}', { {{ initial }} rules: {{ form.rules }} }]"
>
  {% for value, label in form.enum %}
    <a-select-option value="{{ value }}">{{ label }}</a-select-option>
  {% endfor %}
</a-select>
{% include 'form/iframe-entrypoint.html.twig' %}
