<a-rate
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  name="{{ form.name }}"
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  v-decorator="['{{ field }}', { initialValue: {{ form.value }} }]"
  :allow-clear="{{ form.allowClear ? 'true' : 'false' }}"
  :allow-half="{{ form.allowHalf ? 'true' : 'false' }}"
  {% if form.character %}
    character="{{ form.character }}"
  {% endif %}
  {% if form.change %}
    @change="{{ form.change }}"
  {% endif %}
  :count="{{ form.count }}"
  {{ form.attributes|raw }}
></a-rate>
