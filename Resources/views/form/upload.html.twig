<a-input-number
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  size="{{ form.size }}"
  :min="{{ form.min }}"
  :max="{{ form.max }}"
  step="{{ form.step }}"
  placeholder="{{ form.placeholder }}"
  v-decorator="['{{ field }}', { initialValue: `{{ form.value }}`, rules: {{ form.rules }} }]"
></a-input-number>

<a-upload
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  accept="{{ form.accept }}"
  action="{{ form.url }}"
  list-type="{{ form.listType }}"
  :show-upload-list="false"
  :file-list="{{ form.fileListKey }}"
  name="{{ form.flag }}"
  :data="{ file_flag: '{{ form.flag }}' }"
  @click.native="
    persistence_file_md5 = '{{ form.fileMd5Key }}',
    persistence_file_sha1 = '{{ form.fileSha1Key }}',
    persistence_file_list = '{{ form.fileListKey }}'"
  @change="{{ form.change }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ field }}FileList']"
>
  <a-button>
    <a-icon type="upload"></a-icon>
    {{ form.placeholder }}
  </a-button>
</a-upload>