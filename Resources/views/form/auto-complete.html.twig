<a-auto-complete
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  name="{{ form.name }}"
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  size="{{ form.size }}"
  :allow-clear="{{ form.allowClear ? 'true' : 'false' }}"
  :data-source="{{ form.dataSource }}"
  :filter-option="{{ form.filterOption }}"
  {% if form.change %}
    @change="{{ form.change }}"
  {% endif %}
  :auto-focus="{{ form.autoFocus ? 'true' : 'false' }}"
  placeholder="{{ form.placeholder }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ form.key }}', { initialValue: `{{ form.value }}`, rules: {{ form.rules }} }]"
></a-auto-complete>
