<a-radio-group
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ field }}', { initialValue: `{{ form.value }}`, rules: {{ form.rules }} }]"
>
  {% for value, label in form.enum %}
    <a-radio value="{{ value }}">{{ label }}</a-radio>
  {% endfor %}
</a-radio-group>
