<a-input-number
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  name="{{ form.name }}"
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  size="{{ form.size }}"
  :min="{{ form.min }}"
  :max="{{ form.max }}"
  step="{{ form.step }}"
  {% if form.change %}
    @change="{{ form.change }}"
  {% endif %}
  :auto-focus="{{ form.autoFocus ? 'true' : 'false' }}"
  placeholder="{{ form.placeholder }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ form.key }}', { initialValue: `{{ form.value }}`, rules: {{ form.rules }} }]"
></a-input-number>
{% include 'form/iframe-entrypoint.html.twig' with {form: form} %}
