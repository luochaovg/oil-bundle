<a-button
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  type="{{ form.type }}"
  size="{{ form.size }}"
  :block="{{ form.block ? 'true' : 'false' }}"
  :ghost="{{ form.ghost ? 'true' : 'false' }}"
  html-type="{{ form.htmlType }}"
  {% if form.circle %}
    shape="circle"
  {% endif %}
  bsw-data="{{ form.data }}"
  @click="dispatcherByVue"
  {{ form.attributes|raw }}
>
  {{ form.icon|icon|raw }}
  {{ form.label|trans({}, 'twig') }}
  {% if form.selector %}
    {% set var = 'preview_selected_row.length' %}
    {{ '{{ ' ~ var ~ ' ? `(${' ~ var ~ '})` : null }}' }}
  {% endif %}
</a-button>
