{% if form.previewRoute %}
  <a-button
    class="bsw-popup-select"
    :disabled="{{ form.disabled ? 'true' : 'false' }}"
    bsw-data='{"location": "{{ path(form.previewRoute, form.previewArgs) }}"}'
    @click.navite="showIFrameByVue"
  >{{ scaffold.cnf.icon_tag|icon|raw }} {{ form.buttonLabel|trans({}, 'twig') }}</a-button>
{% endif %}