{% set initial = (form.value is same as(null)) ? "null" : ("moment(`" ~ form.value ~ "`, `" ~ form.format ~ "`)") %}
<a-time-picker
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  name="{{ form.name }}"
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  :allow-clear="{{ form.allowClear ? 'true' : 'false' }}"
  size="{{ form.size }}"
  {% if form.change %}
    @change="{{ form.change }}"
  {% endif %}
  :auto-focus="{{ form.autoFocus ? 'true' : 'false' }}"
  placeholder="{{ form.placeholder }}"
  format="{{ form.format }}"
  :hour-step="{{ form.hourStep }}"
  :minute-step="{{ form.minuteStep }}"
  :second-step="{{ form.secondStep }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ field }}', { initialValue: {{ initial }}, rules: {{ form.rules }} }]"
></a-time-picker>
