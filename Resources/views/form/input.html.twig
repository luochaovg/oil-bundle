<a-input
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  type="{{ form.type }}"
  size="{{ form.size }}"
  :allow-clear="{{ form.allowClear ? 'true' : 'false' }}"
  placeholder="{{ form.placeholder }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ field }}', { initialValue: `{{ form.value }}`, rules: {{ form.rules }} }]"
></a-input>