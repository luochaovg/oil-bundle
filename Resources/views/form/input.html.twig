<a-input
  {% if form.class %}
    class="{{ form.class }}"
  {% endif %}
  name="{{ form.name }}"
  :style="{{ form.style }}"
  :disabled="{{ form.disabled ? 'true' : 'false' }}"
  type="{{ form.type }}"
  size="{{ form.size }}"
  :allow-clear="{{ form.allowClear ? 'true' : 'false' }}"
  {% if form.maxLength %}
    :max-length="{{ form.maxLength }}"
  {% endif %}
  placeholder="{{ form.placeholder }}"
  {{ form.attributes|raw }}
  v-decorator="['{{ field }}', { initialValue: `{{ form.value }}`, rules: {{ form.rules }} }]"
>
  {% if form.icon %}
    {{ form.icon|icon(true, form.iconClass, form.iconAttribute)|raw }}
  {% endif %}
</a-input>
{% include 'form/iframe-entrypoint.html.twig' %}
